{
  "name": "admin",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm run build:ui",
    "build:ui": "vite build",
    "dev": "concurrently \"vite build --watch\" \"wrangler dev\"",
    "deploy": "pnpm run build && wrangler deploy",
    "db:migrate": "wrangler d1 migrations apply myshop-d1 --remote",
    "db:migrate:local": "wrangler d1 migrations apply myshop-d1 --local",
    "db:seed": "node scripts/seed.mjs",
    "test": "vitest run"
  },
  "dependencies": {
    "@myshop/shared": "file:./shared",
    "hono": "^4.6.0",
    "jose": "^5.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240208.0",
    "@vitejs/plugin-react": "^4.3.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.0",
    "esbuild": "^0.24.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vite": "^6.0.0",
    "vitest": "^2.0.0",
    "wrangler": "^3.90.0"
  }
}
